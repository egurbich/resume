<div class="m-3 p-3">
    <div class="text-center">
      <h1>{{page_header.title}}</h1>
      <p class="text-uppercase">{{page_header.description}}</p>
    </div>

    <div class="mt-3 p-3 text border rounded">
        <div class="display-6 text-end">{{section.about_us.title_list.main}}</div>
        <h2 class="text-end">{{section.about_us.title_list.sub}}</h2>
        <button type="button" class="btn btn-outline-info">{{section.about_us.button.text}}</button>
    </div>

    <div class="text-secondary">
      <div class="mt-3 p-3 text border rounded">
        <div class="display-5">{{section.advantage.title}}</div>
          <div class="mt-3 p-3 text border rounded">
            <div class="display-4">{{section.advantage.efficiency.title}}</div>
            <h6 class="pt-2">{{section.advantage.efficiency.sub_title}}</h6>
            <p class="pt-2">{{section.advantage.efficiency.description}}</p>
            <button type="button" class="btn btn-outline-warning">{{section.advantage.efficiency.button}}</button>
        </div>
      </div>

      <div class="mt-3 p-3 text border rounded">
        <div class="text-center">
          <div class="display-4">{{section.advantage.innovation.title}}</div>
          <h6 class="pt-2">{{section.advantage.innovation.sub_title}}</h4>
          <p class="pt-2">{{section.advantage.innovation.description}}</p>
        </div>
        <button type="button" class="btn btn-outline-warning">{{section.advantage.innovation.button}}</button>
      </div>

      <div class="mt-3 p-3 text border rounded">
        <div class="text-end">
          <div class="display-4">{{section.advantage.collaboration.title}}</div>
          <h6 class="pt-2">{{section.advantage.collaboration.sub_title}}</h6>
          <p class="pt-2">{{section.advantage.collaboration.description}}</p>
        </div>
        <button type="button" class="btn btn-outline-warning">{{section.advantage.collaboration.button}}</button>
      </div>
    </div>
    <div class="mt-3 p-3 text border rounded">
      <div class="display-5">{{section.agreement.title}}</div>
      <div class="mt-3 p-3 text border rounded">
        <h5 class="p-2">{{section.agreement.info.sub_title}}</h5>
        <div class="mt-3 p-3 text border rounded">
          <p class="p-2">{{section.agreement.info.detail.text}}</p>
          <button type="button" class="btn btn-success">{{section.agreement.info.detail.button}}</button>
        </div>
      </div>
    </div>

    <img class="image" src="https://www.innova-mind.com/wp-content/uploads/2023/10/innova-mind-logo-large.png" alt="InnovaMind logo">
  </div>
  </div>